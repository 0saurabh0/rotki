{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES5",
    "baseUrl": ".",
    "paths": {
      "@/*": ["../../src/*"]
    },
    "types": ["cypress", "node"],
    "inlineSourceMap": true,
    "sourceMap": false
  },
  "include": ["../../src/**/*.ts", "**/*.ts", "../../src/auto-imports.d.ts"],
  "exclude": ["node_modules/**", "**/.*"],
  "compileOnSave": false
}
